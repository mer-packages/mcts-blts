<?xml version="1.0" encoding="ISO-8859-1"?>
<testdefinition version="0.1">
  <suite name="mwts-feedback-scripts" domain="Feedback" level="Feature">
    <set name="Feedback-FuTe" description="Feedback functional test cases" feature="Feedback" type="Functional positive">
      <case name="FUTE-Feedback-StartEffect" subfeature="Vibra" timeout="30" description=
        "Purpose
        Test if effect feature (vibration in mobile devices) works
        Method
        Start effect with simple settings (no attack or fade).
        Pre-/Post-Conditions
        Test on device, DBUS must be running.
        Pass/Fail Criteria
        Test case is successfull, when state of effect switchs to 'Running' and no error occurs.
        ">
        <step>min -c -t 'FUTE-Feedback-StartEffect'</step>
      </case>
  <!--    <case name="FUTE-Feedback-PauseEffect" subfeature="Vibra" timeout="30" description=
        "Purpose
        Test if effect feature (vibration in mobile devices) works
        Method
        Starts effect with simple settings (no attack or fade), and then pauses it.
        Pre-/Post-Conditions
        Test on device, DBUS must be running.
        Pass/Fail Criteria
        Test case is successfull, when state of effect switchs to 'Paused' and no error occurs.
        ">
        <step>min -c -t 'FUTE-Feedback-PauseEffect'</step>
      </case>
