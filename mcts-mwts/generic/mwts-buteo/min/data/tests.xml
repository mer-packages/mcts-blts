<?xml version="1.0" encoding="ISO-8859-1"?>
<testdefinition version="0.1">
  <suite name="mwts-qmsystem-tests" domain="System">
    <set name="FUTE-Sensors" feature="Sensors" type="Functional">
      <case name="FUTE-Sensors_accelerometer" timeout="10" description=
        "Purpose
        Get values from Accelerometer
        Method
        QT Mobility Sensors API call, waiting simultenaus changing values from API callback  
        Pre-/Post-conditions
        must be run as user 
        Pass-/Fail Criteria
        test runs successfully, test does not crash
        ">
        <step>min -c -t 'FUTE-Sensors_accelerometer'</step>
      </case>
