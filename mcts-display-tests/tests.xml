<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
  <description/>
  <suite name="mcts-display-tests">
    <set name="Display function testing">
      <case name="Display-Enumerate%20Screen/Crtc/Output/Modeline%20" manual="false" timeout="90" type="Functional positive">
        <step expected_result="0" manual="false">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Enumerate_display.log" -en "Display-Enumerate Screen/Crtc/Output/Modeline test"</step>
      </case>
      <case name="Display-Set%20Crtc%20modeline" manual="false" timeout="180" type="Functional positive">
        <step expected_result="0" manual="false">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Set_Crtc_Mode.log" -en "Display-Set Crtc Modeline test"</step>
      </case>
      <case name="Display-Set%20Backlight" manual="false" timeout="90" type="Functional positive">
        <step expected_result="0" manual="false">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Set_Backlight.log" -en "Display-Set Backlight test"</step>
      </case>
      <case name="Display-Rotation%20test" manual="false" timeout="90" type="Functional positive">
        <step expected_result="0" manual="false">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Set_Rotation.log" -en "Display-Rotation test"</step>
      </case>
      <case name="Display-Set%20Position" manual="false" timeout="90" type="Functional positive">
        <step expected_result="0" manual="false">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Set_Position.log" -en "Display-Set Crtc Position test"</step>
      </case>
      <case name="Display-Scaling%20test" manual="false" timeout="90" type="Functional positive">
        <step expected_result="0" manual="false">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Set_Scale.log" -en "Display-Crtc Scaling test"</step>
      </case>
      <case name="Display-Hotplug%20test" manual="true" timeout="90" type="Functional positive">
        <step expected_result="0" manual="true">/usr/bin/mcts-display-tests -l "/var/log/tests/mcts-display-tests/Display-Hotplug.log" -en "Display-Hotplug external display"</step>
      </case>
    </set>
  </suite>
</testdefinition>
