<?xml version="1.0" encoding="UTF-8"?>
<testdefinition version="1.0">
  <suite name="mcts-openvg-tests">
    <set name="openvg-function and perf">
      <case name="OpenVG-Enumerate%20EGL%20extensions" manual="false" timeout="90" requirement="no" type="Functional positive">
        <description><![CDATA[<p>Lists all EGL extensions</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Enumerate_EGL_extensions.log" -en "OpenVG - Enumerate EGL extensions"</step>
      </case>
      <case name="OpenVG-Enumerate%20EGL%20configs" manual="false" timeout="90" type="Functional positive">
        <description><![CDATA[<p>Lists all EGL configurations</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Enumerate_EGL_configs.log" -en "OpenVG - Enumerate EGL configs"</step>
      </case>
      <case name="OpenVG-Transformation%20perf" manual="false" timeout="90" type="Performance">
        <description><![CDATA[<p>Transformation function and perf testing</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Transformation_testing.log" -en "OpenVG - Transformation testing"</step>
      </case>
      <case name="OpenVG-Stroking%20perf" manual="false" timeout="90" type="Performance">
        <description><![CDATA[<p>Stroking function and perf testing</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Stroking_Path_testing.log" -en "OpenVG - Stroking Path testing"</step>
      </case>
      <case name="OpenVG-Filling%20perf" manual="false" timeout="90" type="Performance">
        <description><![CDATA[<p>Filling function and perf test with two models (VG_NON_ZERO/VG_EVEN_ODD).</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Filling_Path_testing.log" -en "OpenVG - Filling Path testing"</step>
      </case>
      <case name="OpenVG-Clearing%20perf" manual="false" timeout="90" type="Performance">
        <description><![CDATA[<p>Fast clearing test</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Clearing_testing.log" -en "OpenVG - Clearing testing"</step>
      </case>
      <case name="OpenVG-Linear%20Gradient%20perf" manual="false" timeout="90" type="Performance">
        <description><![CDATA[<p>Linear Gradient function and perf Testing with thtree SPREAD models (PAD/REPEAT/REFLECT)&quot;</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Linear_Gradients_testing.log" -en "OpenVG - Linear Gradients testing"</step>
      </case>
      <case name="OpenVG-Radial%20Gradient%20perf" manual="false" timeout="90" type="Performance">
        <description><![CDATA[<p>Radial Gradient function and perf Testing with thtree SPREAD models (PAD/REPEAT/REFLECT)</p>]]></description>
        <step expected_result="0" manual="false">/usr/bin/blts-openvg-tests -l "/var/log/tests/blts-openvg-tests/OpenVG-Radial_Gradients_testing.log" -en "OpenVG - Radial Gradients testing"</step>
      </case>
      <get>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Enumerate_EGL_extensions.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Enumerate_EGL_configs.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Transformation_testing.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Stroking_Path_testing.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Filling_Path_testing.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Clearing_testing.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Linear_Gradients_testing.log"</file>
        <file>"/var/log/tests/blts-openvg-tests/OpenVG-Radial_Gradients_testin.log"</file>
      </get>
    </set>
  </suite>
</testdefinition>
